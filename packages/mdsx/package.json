{
  "name": "@ap0nia/mdsx2",
  "private": true,
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js",
      "require": "./src/*.js",
      "default": "./src/*.js"
    },
    "./*.svelte": {
      "svelte": "./src/*.svelte"
    }
  },
  "dependencies": {},
  "devDependencies": {}
}

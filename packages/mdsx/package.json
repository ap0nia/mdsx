{
  "name": "@ap0nia/mdsx2",
  "private": true,
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js",
      "require": "./src/*.js",
      "default": "./src/*.js"
    },
    "./*.svelte": {
      "svelte": "./src/*.svelte"
    }
  },
  "dependencies": {
    "vfile": "^6.0.3",
    "yaml": "^2.6.0",
    "zimmerframe": "^1.1.2"
  }
}

{
  "name": "@ap0nia/mdsx",
  "private": true,
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js",
      "require": "./src/*.js",
      "default": "./src/*.js"
    },
    "./*.svelte": {
      "svelte": "./src/*.svelte"
    }
  },
  "dependencies": {
    "@shikijs/twoslash": "^1.22.2",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-markdown": "^2.1.2",
    "svelte": "^5.1.11",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3",
    "yaml": "^2.6.0",
    "zimmerframe": "^1.1.2"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "shiki": "^1.22.2"
  }
}
